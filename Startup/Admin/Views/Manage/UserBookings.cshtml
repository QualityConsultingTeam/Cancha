
@{
    Layout = null;
}

@model List<Booking>
<table class="table table-striped table-hover">
    <tbody>

        @foreach (var booking in Model)
        {
            <tr>
                @*<td class="client-avatar"><img alt="image" src="@Html.FacebookProfileSmallPicture()"> </td>*@
                <td><a data-toggle="tab" href="#contact-1" class="client-link">@booking.Start</a></td>
                <td> @booking.Field.Name</td>
                <td> @booking.Field.Center.Name</td>
                <td class="contact-type"><i class="fa fa-plus-circle"> </i></td>
                <td> @Html.DisplayFor(m=> booking.Price)</td>
                <td class="contact-type"><i class="fa fa-paypal"> </i></td>
                <td> @booking.PaypalPaymentId</td>
                <td class="client-status"><span class="label label-primary">@booking.Status</span></td>
            </tr>
        }
        
       
    </tbody>
</table>
