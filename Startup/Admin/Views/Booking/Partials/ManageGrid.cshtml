
@{
    Layout = null;
}

@model IEnumerable<Booking>

   

<table class="footable table table-stripped toggle-arrow-tiny animate fadeInLeft"  >
    <thead>
        <tr>

            <th>Hora</th>
            <th>Complejo</th>
            <th data-hide="phone">Cliente</th>
            <th data-hide="phone">Info</th>
            <th data-hide="phone">Estado</th>
            
            <th class="text-right">Accion</th>

        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem=> item.Start)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Field.Name)
                </td>
                <td>
                    <button class="btn btn-info btn-xs" onclick="ShowUserDetails('@item.UserInfo.Id')">
                        @item.UserInfo.Email
                    </button>
                </td>
                <td>
                    @item.Type
                </td>
                <td>
                    <span class="label label-primary">  @item.Status</span>
                </td>
            
           
                <td class="text-right">
                    <div class="btn-group">
                        @if (item.Status == BookingStatus.Pendiente)
                        {
                            <button class="btn-primary btn btn-xs" onclick="ConfirmBookingAction('@item.Id')">Confirmar</button>
                        }
                        else if (item.Status == BookingStatus.Confirmada)
                        {
                            <button class="btn-danger btn btn-xs" onclick=" cancelbookingaction('@item.Id') ">Cancenlar</button>
                        }
                        else
                        {
                            @Html.Label("Finalizada")
                        } 
                        @*<button class="btn-white btn btn-xs">Edit</button>
                    <button class="btn-white btn btn-xs">Delete</button>*@
                    </div>
                </td>
            </tr>
        }
