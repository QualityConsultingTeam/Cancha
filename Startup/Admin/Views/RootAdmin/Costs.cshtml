@model IEnumerable<Access.Models.Cost>

@{
    Layout = null;
}

 

 
    <div class="wrapper wrapper-content animated fadeInRight">
        <div class="row">
            <div class="col-lg-12">
                <div class="ibox float-e-margins">
                    <div class="ibox-title">
                        <h5>List of Cost</h5>
    					<div class="ibox-tools">
    						@Html.ActionLink("Crear nuewo", "Create", null, new { @class = "btn btn-primary btn-xs"})
    					</div>
                    </div>
                    <div class="ibox-content">
                        <div class="table-responsive">
                            <table class="table table-striped">
                                <tr>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Opentime)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Closetime)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Price)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.DownPayment)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Dayofweek)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.IdCancha)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.UserSign)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.CreateDate)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.CreateTime)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.OBJECTTYPE)
                                    </th>
                                    <th></th>
                                </tr>

                                @foreach (var item in Model) {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.Opentime)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Closetime)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Price)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.DownPayment)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Dayofweek)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.IdCancha)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.UserSign)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.CreateDate)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.CreateTime)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.OBJECTTYPE)
                        </td>
                        <td>
                            @Html.ActionLink("Details", "Details", new { id=item.Id }, new { @class = "btn btn-primary btn-sm"})
                            @Html.ActionLink("Edit", "Edit", new { id=item.Id }, new { @class = "btn btn-white btn-sm"})
                            @Html.ActionLink("Delete", "Delete", new { id=item.Id }, new { @class = "btn btn-white btn-sm"})
                        </td>
                    </tr>
    }

                            </table>

                        </div>
                </div>
            </div>
        </div>
     </div>
    </div>
    
  
