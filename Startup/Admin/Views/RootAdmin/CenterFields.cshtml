@model IEnumerable<Access.Models.Field>

@{
    Layout = null;
}

 
    
    <div class="wrapper wrapper-content animated fadeInRight">
        <div class="row">
            <div class="col-lg-12">
                <div class="ibox float-e-margins">
                    <div class="ibox-title">
                        <h5>List of Field</h5>
    					<div class="ibox-tools">
    						@Html.ActionLink("Agregar", "CenterEdit", null, new { @class = "btn btn-primary btn-xs"})
    					</div>
                    </div>
                    <div class="ibox-content">
                        <div class="table-responsive">
                            <table class="table table-striped">
                                <tr>
                                    <th>
                                        @Html.DisplayNameFor(model => model.UserSign)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.FieldPicure)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Name)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.CenterId)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Location)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Description)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Comments)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Town)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Department)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Country)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Neighborhood)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Length)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Width)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Grade)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.CreateDate)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.CreateTime)
                                    </th>

                                    <th>
                                        @Html.DisplayNameFor(model => model.OBJECTTYPE)
                                    </th>
                                    <th></th>
                                </tr>

                                @foreach (var item in Model) {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.UserSign)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.FieldPicure)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Name)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.CenterId)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Location)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Description)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Comments)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Town)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Department)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Country)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Neighborhood)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Length)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Width)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Grade)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.CreateDate)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.CreateTime)
                        </td>

                        <td>
                            @Html.DisplayFor(modelItem => item.OBJECTTYPE)
                        </td>
                        <td>
                            @Html.ActionLink("Detalles", "FieldDetails", new { fieldId = item.Id }, new { @class = "btn btn-primary btn-sm" })
                            @Html.ActionLink("Editar", "EditField", new { id = item.Id }, new { @class = "btn btn-white btn-sm" })
                            @*@Html.ActionLink("Eliminar", "Delete", new { id=item.Id }, new { @class = "btn btn-white btn-sm"})*@
                        </td>
                    </tr>
    }

                            </table>

                        </div>
                </div>
            </div>
        </div>
     </div>
    </div>
    
    
 